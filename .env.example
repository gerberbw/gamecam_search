# YOLO model name or path (e.g. yolov8n.pt or a custom weights file)
YOLO_MODEL=yolov8n.pt

# Comma-separated labels to match (required). Example: person,car,cat
YOLO_LABELS=person,car

# Minimum confidence threshold for detections (0.0 - 1.0)
CONFIDENCE_THRESHOLD=0.25

# Whether to search subdirectories (true/false)
RECURSIVE=true
